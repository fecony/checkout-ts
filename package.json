{
  "name": "checkout-api",
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.11",
    "@types/xo": "^0.39.9",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.3",
    "xo": "^0.59.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*": "xo --fix"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "sideEffects": false,
  "simple-git-hooks": {
    "pre-commit": "bun run lint-staged"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  }
}
